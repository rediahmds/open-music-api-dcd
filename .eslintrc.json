{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "no-underscore-dangle": ["error", { "allowAfterThis": true }],
      "comma-dangle": [
        "error",
        {
          "functions": "never",
          "objects": "always-multiline",
          "arrays": "always-multiline"
        }
      ]
    }
}
